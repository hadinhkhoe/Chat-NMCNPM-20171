                    <!--<div class="userLine">Tran Quang Khai</div>-->
                </div>
            </div>

            <div id="right">
                <div id="sayHi">
                    Hello <span id="currentUser">Teo</span>
                    <input type="button" id="btnLogout" value="Logout">
                </div>


                <div id="listMessages">

                </div>
                <div id="status">**********************</div>

                <input type="text" id="txtMessages" maxlength="100">
